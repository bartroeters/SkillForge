{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "es2022",
        "outDir": "./built",
        "allowJs": true,
        "target": "es2022",
        "allowUnreachableCode": false,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "typeRoots": ["./typings"],
        "lib": ["es2017", "DOM"],
        "moduleResolution": "node",
        "baseUrl": "./resources/js",
        "paths": {
            "@/*": ["./*"],
            "components/*": ["./components/*"],
            "domains/*": ["./domains/*"],
            "helpers/*": ["./helpers/*"],
            "routes/*": ["./routes/*"],
            "services/*": ["./services/*"]
        }
    },
"include": ["./**/*"],
"exclude": ["./plugins/**/*", "./typings/**/*", "./built/**/*", "node_modules"]
}
